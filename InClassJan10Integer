#include <iostream>
#include <cmath>

using namespace std;

int main() {
int int1;
int int2;
cout << "Enter an integer";
cin >> int1;
cout << "Enter another interger";
cin >> int2;
if (int1 > int2) {
    cout << int1; " is larger";
}
if (int2 > int1) {
    cout << int2; " is larger";
}
else
{
    cout << "integers are equal";
}

    return 0;
}
